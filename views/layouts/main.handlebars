<!-- Section Location -->
<section id="location">
  <div id="google-container"></div>
  <div id="cd-zoom-in"></div>
  <div id="cd-zoom-out"></div>
  <address>
  Paralelní Polis, Dělnická 43, Praha 7, Czech Republic
  </address>
</section>
<!-- /.Section Location -->

<!-- Section Contact -->
<section id="contact">
  <div class="container">
    <!-- Title -->
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h2 class="text-center"><span>Contact us</span></h2>
          <p class="subtitle text-center">
            Do you have any questions regarding HCPP18? Send us an e-mail and we will reach you soon.
          </p>
        </div>
      </div>
    </div>
    <!-- /.Title -->

    {{#if contact_message}}
      <div class="subtitle text-center">
        {{contact_message}}
      </div>
    {{else}}
    <form action="/contact" method="POST" enctype="multipart/form-data" id="contactForm">
      <div class="row">
        <div class="col-md-6">
          <div class="form-group mrgn-10-btm">
            <label for="name" class="control-label">Name *</label>
            <input type="text" name="name" class="form-control" id="name" placeholder="Name" required>
          </div>
          <div class="form-group mrgn-10-btm">
            <label for="email" class="control-label">Email *</label>
            <input type="email" name="email" class="form-control" id="email" placeholder="Email" required>
          </div>
          <div class="form-group mrgn-10-btm">
            <label for="subject" class="control-label">Subject *</label>
            <input type="text" name="subject" class="form-control" id="subject" placeholder="Subject" required>
          </div>
          <div class="form-group mrgn-10-btm">
            <label for="company" class="control-label">Company</label>
            <input type="text" name="company" class="form-control" id="company" placeholder="Company">
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group mrgn-10-btm">
            <label for="message" class="control-label">Your message *</label>
            <textarea id="message" name="message" rows="13" class="form-control" required></textarea>
          </div>
          <div class="clearfix mrgn-10-btm">
            <div class="pull-right">
              {{{captcha}}}
            </div>
          </div>
        </div>
      </div>
      <button type="submit" class="btn btn-main-color pull-right">Send</button>
    </form>
    {{/if}}
  </div>
</section>
<!-- /.Section Contact -->

