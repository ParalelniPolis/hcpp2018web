<!-- Schedule Timeline -->
{{>schedule}}
<!-- /.Schedule Timeline -->
<ul class="juicer-feed" data-feed-id="hcpp17" data-columns="4"></ul>
<style>
  body {
    transform: scale(0.4);
    transform-origin: top;
    position: relative;
  }
  #darknav.navbar {
    display: none;
  }
  #schedule {
    padding: 0;
    background-color: #fff;
    transform: translateX(-50%);
    position: absolute;
    top: 0;
    opacity: 0;
  }
  #schedule .container {
    width: 200%;
  }
  #location,
  #contact,
  footer {
    display: none;
  }
  .juicer-feed.loaded {
    width: 200%;
    transform: translateX(-25%);
    position: absolute;
    top: 0;
  }
</style>
<script>
  var feed = 'social';
  setTimeout(function() {
  	window.location.reload();
  }, 300000);

  setInterval(function() {
  	if(feed === 'social') {
      $('.juicer-feed.loaded').animate({ opacity: 0 }, 3000);
      $('#schedule').animate({ opacity: 1 }, 3000);
      feed = 'schedule';
    }
    else {
      $('.juicer-feed.loaded').animate({ opacity: 1 }, 3000);
      $('#schedule').animate({ opacity: 0 }, 3000);
      feed = 'social';
    }
  }, 30000);
</script>
<script src="//assets.juicer.io/embed.js" type="text/javascript"></script>
<link href="//assets.juicer.io/embed.css" media="all" rel="stylesheet" type="text/css" />
