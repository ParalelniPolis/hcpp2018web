<section id="full-schedule">
  <div class="container">
    <!-- Title & Subtitle -->
    <div class="row">
      <div class="col-lg-12">
        <div class="section-title">
          <h2 class="text-center"><span>Full Schedule</span></h2>
          <p class="subtitle text-center"><i class="fa fa-angle-down fa-lg"></i></p>
        </div>
      </div>
    </div>
    <!-- /.Title & Subtitle -->

    <!-- Schedule Content -->
    <div class="row">
      <div class="col-lg-12">

        <!-- Days Schedule  -->
        <div class="row">
          <div class="col-lg-10 col-lg-offset-1">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs nav-justified" role="tablist" id="toolkit">
              <li><a href="#day1" role="tab" data-toggle="tab"><span class="text-bold text-big ">Day 01</span><br>
                <span class="small">[06 October]</span></a>
              </li>
              <li><a href="#day2" role="tab" data-toggle="tab"><span class="text-bold text-big ">Day 02</span><br>
                <span class="small">[07 October]</span></a>
              </li>
              <li class="active"><a href="#day3" role="tab" data-toggle="tab"><span class="text-bold text-big ">Day 03</span><br>
                <span class="small">[08 October]</span></a>
              </li>
            </ul>
            <!-- /.Nav tabs -->

            <!-- Tabs Panes Days -->
            <div class="tab-content">
              <!-- Tab - Day 01 -->
              <div class="tab-pane fade" id="day1">
                <div class="coffee-break white">
                  <div class="row">
                    <p>
                      9.00<br>
                      Opening
                    </p>
                  </div>
                </div>
                <hr class="border-light">
                {{#forEach day1}}
                <!-- Talk -->
                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">{{moment starts 'HH.mm A'}}</p>
                    {{#each speakers}}
                      <p><img src="/media/images/transparent.png" data-lazy="{{photo.url}}" alt="{{displayName}}" class="img-responsive center-block img-circle img-speaker">
                        {{displayName}}
                      </p>
                    {{/each}}
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">{{name}}</p>
                    <p>{{{breaklines description}}}</p>
                    <span class="label label-main">{{room.name}}</span></div>
                </div>
                <!-- /.Talk -->
                {{#unless isLast}}<hr class="border-light">{{/unless}}
                {{/forEach}}
              </div>
              <!-- /.Tab - Day 01 -->

              <!-- Tab - Day 02 -->
              <div class="tab-pane fade" id="day2">
                <div class="coffee-break white">
                  <div class="row">
                    <p>
                      9.00<br>
                      Opening
                    </p>
                  </div>
                </div>
                <hr class="border-light">
                {{#forEach day2}}
                <!-- Talk -->
                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">{{moment starts 'HH.mm A'}}</p>
                    {{#each speakers}}
                      <p><img src="/media/images/transparent.png" data-lazy="{{photo.url}}" alt="{{displayName}}" class="img-responsive center-block img-circle img-speaker">
                        {{displayName}}
                      </p>
                    {{/each}}
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">{{name}}</p>
                    <p>{{{breaklines description}}}</p>
                    <span class="label label-main">{{room.name}}</span></div>
                </div>
                <!-- /.Talk -->
                {{#unless isLast}}<hr class="border-light">{{/unless}}
                {{/forEach}}
              </div>
              <!-- /.Tab - Day 02 -->

              <!-- Tab - Day 03 -->
              <div class="tab-pane fade active in" id="day3">
                <div class="coffee-break white">
                  <div class="row">
                    <p>
                      9.00<br>
                      Opening
                    </p>
                  </div>
                </div>
                <hr class="border-light">
                {{#forEach day3}}
                <!-- Talk -->
                <div class="row">
                  <div class="col-xs-4 col-sm-2 text-center">
                    <p class="text-big text-bold">{{moment starts 'HH.mm A'}}</p>
                    {{#each speakers}}
                      <p><img src="/media/images/transparent.png" data-lazy="{{photo.url}}" alt="{{displayName}}" class="img-responsive center-block img-circle img-speaker">
                        {{displayName}}
                      </p>
                    {{/each}}
                  </div>
                  <div class="col-xs-8 col-sm-10">
                    <p class="text-big text-bold text-uppercase">{{name}}</p>
                    <p>{{{breaklines description}}}</p>
                    <span class="label label-main">{{room.name}}</span></div>
                </div>
                <!-- /.Talk -->
                {{#unless isLast}}<hr class="border-light">{{/unless}}
                {{/forEach}}
              </div>
              <!-- /.Tab - Day 03 -->

            </div>
            <!-- /.Tabs Panes Days -->
          </div>
        </div>
        <!-- /.Days Schedule  -->
      </div>
    </div>
    <!-- /.Schedule Content -->
  </div>
  <!-- /.container -->
</section>
